{
    "logger": {
        // The tank for logger, console or file.
        "tank": "file",
        // The log file path, only for tank file.
        "file": "./apilb.log"
    },
    "backend": {
        // Whether enable the backend api.
        "enabled": true,
        // The listen tcp4 or tcp6 addrs for backend,
        // for example, to proxy to srs server api.
        //      tcp://:1985, tcp://0.0.0.0:1985, tcp4://:1985, tcp6://:1985
        "api": "tcp://:1985"
    },
    // The control api listen tcp4 or tcp6 addrs, for example,
    // tcp://127.0.0.1:2039, tcp4://127.0.0.1:2039
    "api": "tcp://:2039"
}
